{
  "name": "php127/websiteinfo",
  "description": "从html代码中分析网站信息",
  "keywords": [
    "网站信息",
    "获取网站信息",
    "备案查询",
    "分析网站首页",
    "获取网站资料"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "读心印",
      "email": "839024615@qq.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "jaeger/querylist": "~V4"
  },
  "autoload": {
    "psr-4": {
      "Php127\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Php127\\Tests\\": "./tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^2.18"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --colors=always --testdox",
    "check-style": "php-cs-fixer fix --using-cache=no --diff --config=.php_cs --dry-run --ansi",
    "fix-style": "php-cs-fixer fix --using-cache=no --config=.php_cs --ansi"
  }
}